{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "image.openshift.io/v1",
            "metadata": {
                "name": "s2i-minimal-notebook-s3",
                "labels": {
                    "build": "s2i-minimal-notebook-s3"
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "build.openshift.io/v1",
            "metadata": {
                "name": "s2i-minimal-notebook-s3",
                "labels": {
                    "build": "s2i-minimal-notebook-s3"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/ulaval/valeria-jupyter-notebooks-s3",
                        "ref": "master"
                    },
                    "contextDir": "minimal-notebook"
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "centos/python-36-centos7",
                            "namespace": "openshift"
                        }
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "s2i-minimal-notebook-s3:prod"
                    }
                },
                "resources": {
                    "limits": {
                        "memory": "1Gi"
                    }
                }
            }
        }
    ]
}
